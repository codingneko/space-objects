<a href="<%= referer %>" class="btn btn-primary mb-4">Back to catalog</a>

<% for (const object of data){ %>
<div class="card">
    <h4 class="ml-4 mt-4"> <%= object.OBJECT_NAME %> </h4>
    <pre class="mb-3"> 
        Type: <%= object.OBJECT_TYPE.toLowerCase() %> 
        International object designator: <%= object.INTLDES %>

        Day of Epoch: <%= object.epochDay %>

        Orbital data:
            --- inclination (degrees) <%= object.inclination %>
            --- right ascension of the ascending node (degrees): <%= object.ascendingNode %>
            --- Argument of perigee (degrees): <%= object.perigee %>
            --- Mean anomaly (degrees): <%= object.anomaly %>
            --- Eccentricity (decimal point assumed): <%= object.eccentricity %>
        
        Revolutions per day (mean motion): <%= object.motion %>
        Revolution count at epoch: <%= object.revAtEpoch %>
    </pre>
    <div class="container">
        <div class="card border-danger mb-3">
            <div class="card-header" id="headingOne">
                <h2 class="mb-0">
                    <button class="btn btn-danger btn-smbtn-danger" data-toggle="collapse" data-target="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                    <span id="toggle"> Show </span> TLE data 
                </button>
                </h2>
            </div>

            <div id="collapseOne" class="collapse" aria-labelledby="headingOne">
                <div class="card-body">
                    <pre class="mt-4">
                        TLE line 1: <%= object.TLE_LINE1 %>
                        TLE line 2: <%= object.TLE_LINE2 %>
                    </pre>
                </div>
            </div>
        </div>
    </div>
</div>
<% } %>