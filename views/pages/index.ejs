<h1>Objects</h1>

<%- include('../inc/search')%>

<% if(typeof data != 'string'){%>
<ul class="list-group mt-4 mb-4">
    <% for (var object of data) { %>
      <li class="list-group-item">
          <h4> <a href="/object/<%= object.id %>"><%= object.name %></a></h4>
          <pre class="mb-3"> 
            Type: <%= object.type.toLowerCase() %> 
            International object designator: <%= object.id %>
            Launch Date: <%= object.date.toDateString() %>
          </pre>
      </li>
    <% } %>
</ul>
<%} else {%>
  <p class="card mt-4"> No data found </p>
<% } %>

<%- include('../inc/pagination'); %>